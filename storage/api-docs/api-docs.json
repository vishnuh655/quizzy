{
    "openapi": "3.0.0",
    "info": {
        "title": "Quizzy API",
        "version": "0.1"
    },
    "paths": {
        "/api/questions/{questionId}": {
            "get": {
                "summary": "Get Question By Id",
                "description": "Get Question By Id",
                "operationId": "dfea6dd8e01e4d6900ca8705a77bb86f",
                "parameters": [
                    {
                        "name": "questionId",
                        "in": "path",
                        "description": "ID of question that needs to be fetched",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation"
                    },
                    "400": {
                        "description": "Invalid tag value"
                    }
                },
                "security": [
                    {
                        "petstore_auth": [
                            "write:pets",
                            "read:pets"
                        ]
                    }
                ]
            }
        },
        "/api/questions": {
            "get": {
                "summary": "Get All Questions",
                "description": "Get list of all questions",
                "operationId": "findPetsByTags",
                "responses": {
                    "200": {
                        "description": "successful operation"
                    },
                    "400": {
                        "description": "Invalid tag value"
                    }
                },
                "security": [
                    {
                        "petstore_auth": [
                            "write:pets",
                            "read:pets"
                        ]
                    }
                ]
            },
            "post": {
                "summary": "Create a question",
                "description": "Create a question",
                "operationId": "findPetsByTags",
                "requestBody": {
                    "description": "Data required for creating the question",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "questions": {
                                        "type": "array",
                                        "items": {
                                            "properties": {
                                                "content": {
                                                    "type": "string",
                                                    "example": "Which popular video game franchise has released games with the subtitles World At War and Black Ops?"
                                                },
                                                "typeId": {
                                                    "type": "integer",
                                                    "example": "1"
                                                }
                                            },
                                            "type": "object"
                                        }
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "successful operation"
                    },
                    "400": {
                        "description": "Invalid tag value"
                    }
                },
                "security": [
                    {
                        "petstore_auth": [
                            "write:pets",
                            "read:pets"
                        ]
                    }
                ]
            }
        }
    }
}